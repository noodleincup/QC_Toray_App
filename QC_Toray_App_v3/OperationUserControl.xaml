                    <TextBox x:Name="tbxBlackSpot_SS_S2" Margin="5,1" Foreground="Black" TextAlignment="Center" InputScope="Number"></TextBox>
                    <TextBox x:Name="tbxBlackSpot_S_S2" Margin="5,1" Foreground="Black" TextAlignment="Center" InputScope="Number"></TextBox>
                    <TextBox x:Name="tbxBlackSpot_M_S2" Margin="5,1" Foreground="Black" TextAlignment="Center" InputScope="Number"></TextBox>
                    <TextBox x:Name="tbxBlackSpot_L_S2" Margin="5,1" Foreground="Black" TextAlignment="Center" InputScope="Number"></TextBox>
                    <TextBox x:Name="tbxColorAbnormal_S2" Margin="5,1" Foreground="Black" TextAlignment="Center" InputScope="Number"></TextBox>
                    <TextBox x:Name="tbxMacaroni_SS_S2" Margin="5,1" Foreground="Black" TextAlignment="Center" InputScope="Number"></TextBox>

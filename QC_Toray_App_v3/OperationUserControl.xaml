                    <TextBox x:Name="txbGF3_S1" Margin="5,1" Foreground="Black" TextAlignment="Center" InputScope="Number"></TextBox>
                    <TextBox x:Name="tbxMeya_S1" Margin="5,1" Foreground="Black" TextAlignment="Center" InputScope="Number"></TextBox>
                    <TextBox x:Name="tbxMeya_NoChg_S1" Margin="5,1" Foreground="Black" TextAlignment="Center" InputScope="Number"></TextBox>
                    <TextBox x:Name="tbxForeignMat_S1" Margin="5,1" Foreground="Black" TextAlignment="Center" InputScope="Number"></TextBox>
                    <TextBox x:Name="tbxBlackSpot_SS_S1" Margin="5,1" Foreground="Black" TextAlignment="Center" InputScope="Number"></TextBox>
                    <TextBox x:Name="tbxBlackSpot_S_S1" Margin="5,1" Foreground="Black" TextAlignment="Center" InputScope="Number"></TextBox>
                    <TextBox x:Name="tbxBlackSpot_M_S1" Margin="5,1" Foreground="Black" TextAlignment="Center" InputScope="Number"></TextBox>
                    <TextBox x:Name="tbxBlackSpot_L_S1" Margin="5,1" Foreground="Black" TextAlignment="Center" InputScope="Number"></TextBox>
                    <TextBox x:Name="tbxColorAbnormal_S1" Margin="5,1" Foreground="Black" TextAlignment="Center" InputScope="Number"></TextBox>
                    <TextBox x:Name="tbxMacaroni_SS_S1" Margin="5,1" Foreground="Black" TextAlignment="Center" InputScope="Number"></TextBox>
                    <TextBox x:Name="tbxMacaroni_S_S1" Margin="5,1" Foreground="Black" TextAlignment="Center" InputScope="Number"></TextBox>
                    <TextBox x:Name="tbxMacaroni_M_S1" Margin="5,1" Foreground="Black" TextAlignment="Center" InputScope="Number"></TextBox>
                    <TextBox x:Name="tbxMacaroni_L_S1" Margin="5,1" Foreground="Black" TextAlignment="Center" InputScope="Number"></TextBox>
                </UniformGrid>

                <!--Sample2-->
                <UniformGrid Grid.Column="3" Grid.RowSpan="24" Columns="1" Rows="24">
                    <TextBlock HorizontalAlignment="Center" >Sample 2</TextBlock>
                    <ToggleButton x:Name="tgPalletSize_S2" Background="LightGreen"></ToggleButton>
                    <TextBox x:Name="txbMissDb_S2" Margin="5,1" Foreground="Black" TextAlignment="Center" InputScope="Number"></TextBox>
                    <TextBox x:Name="txbMissTp_S2" Margin="5,1" Foreground="Black" TextAlignment="Center" InputScope="Number"></TextBox>
                    <TextBox x:Name="txbMissFf_S2" Margin="5,1" Foreground="Black" TextAlignment="Center" InputScope="Number"></TextBox>
                    <TextBox x:Name="txbLinkDb_S2" Margin="5,1" Foreground="Black" TextAlignment="Center" InputScope="Number"></TextBox>
                    <TextBox x:Name="txbLinkTp_S2" Margin="5,1" Foreground="Black" TextAlignment="Center" InputScope="Number"></TextBox>
                    <TextBox x:Name="txbLinkFf_S2" Margin="5,1" Foreground="Black" TextAlignment="Center" InputScope="Number"></TextBox>
                    <TextBox x:Name="txbLinkFF_S2" Margin="5,1" Foreground="Black" TextAlignment="Center" InputScope="Number"></TextBox>
                    <TextBox x:Name="txbDefect_S2" Margin="5,1" Foreground="Black" TextAlignment="Center" InputScope="Number"></TextBox>
                    <TextBox x:Name="txbGF1_S2" Margin="5,1" Foreground="Black" TextAlignment="Center" InputScope="Number"></TextBox>
                    <TextBox x:Name="txbGF2_S2" Margin="5,1" Foreground="Black" TextAlignment="Center" InputScope="Number"></TextBox>
                    <TextBox x:Name="txbGF3_S2" Margin="5,1" Foreground="Black" TextAlignment="Center" InputScope="Number"></TextBox>

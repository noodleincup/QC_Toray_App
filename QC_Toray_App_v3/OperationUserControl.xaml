                <UniformGrid Grid.Column="4" Grid.RowSpan="24" Columns="1" Rows="24">
                    <TextBlock HorizontalAlignment="Center" >Sample 3</TextBlock>
                    <ToggleButton x:Name="tgPalletSize_S3" Background="LightGreen"></ToggleButton>
                    <TextBox x:Name="txbMissDb_S3" Margin="5,1" Foreground="Black" TextAlignment="Center" InputScope="Number"></TextBox>
                    <TextBox x:Name="txbMissTp_S3" Margin="5,1" Foreground="Black" TextAlignment="Center" InputScope="Number"></TextBox>

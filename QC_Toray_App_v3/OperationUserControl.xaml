                    <TextBox x:Name="tbxMacaroni_M_S2" Margin="5,1" Foreground="Black" TextAlignment="Center" InputScope="Number"></TextBox>
                    <TextBox x:Name="tbxMacaroni_L_S2" Margin="5,1" Foreground="Black" TextAlignment="Center" InputScope="Number"></TextBox>
                </UniformGrid>

